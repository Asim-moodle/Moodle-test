<XMLDB PATH="local/csvemail/db" VERSION="2024102200" COMMENT="CSV Email Sender log">
    <TABLES>
        <TABLE NAME="csvemail_log" COMMENT="Email log table">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="firstname" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="lastname" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="email" TYPE="char" LENGTH="255" NOTNULL="true"/>
                <FIELD NAME="date_sent" TYPE="int" LENGTH="10" UNSIGNED="true" NOTNULL="true"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
